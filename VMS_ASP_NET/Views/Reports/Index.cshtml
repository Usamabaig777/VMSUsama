@model VMS_ASP_NET.Models.Reports
@using System.Data
@using VMS_ASP_NET.Models
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    List<IncidentReport> incidentReportsList = ViewBag.IncidentReports;
    UserRole userRole = (UserRole)Session["userRole"];
}

<div class="container-fluid">
    <div class="animated fadeIn">
        <ul class="nav nav-tabs">
            <li class="nav-item"><a class="nav-link active" href="#FirstTab" data-toggle="tab"><i class="fa fa-bar-chart"></i> <span class=""> Incident</span></a></li>
            <li class="nav-item"><a class="nav-link" href="#SecondTab" data-toggle="tab"><i class="fa fa-area-chart"></i> <span class="d-none"> Operational</span></a></li>
        </ul>
        <div class="tab-content">
            <div class="tab-pane active" id="FirstTab">
                <div class="row">
                    <div class="col">
                        <div class="card">
                            <div class="pt-0 pr-0 pl-0 pb-0 card-body">
                                <div class="animated fadeIn">
                                    <div class="row">
                                        <div class="col-12 col-lg-12">
                                            <div class="card">
                                                <div class="card-header">
                                                    <div class="clearfix" style="padding: 0.5rem;">
                                                        @if (userRole.Role.RoleName == "ExternalAdmin")
