                                                                    <div class="rt-th" role="columnheader" tabindex="-1" style="flex: 200 0 auto; width: 200px; max-width: 200px;"></div>
                                                                    <div class="rt-th" role="columnheader" tabindex="-1" style="flex: 100 0 auto; width: 100px;"></div>
                                                                    <div class="rt-th" role="columnheader" tabindex="-1" style="flex: 100 0 auto; width: 100px;"></div>
                                                                    <div class="rt-th" role="columnheader" tabindex="-1" style="flex: 100 0 auto; width: 100px; max-width: 100px;"></div>
                                                                    <div class="rt-th" role="columnheader" tabindex="-1" style="flex: 80 0 auto; width: 80px; max-width: 80px;"></div>
                                                                    <div class="rt-th" role="columnheader" tabindex="-1" style="flex: 0 0 auto; width: 0px; max-width: 0px;">0</div>
                                                                </div>
                                                            </div>
                                                            <div class="rt-tbody" style="min-width: 730px;">
                                                                @{
                                                                    if (incidentReportsList != null)
                                                                    {
                                                                        foreach (var item in incidentReportsList)
                                                                        {
                                                                            <div class="rt-tr-group" role="rowgroup">
                                                                                <div class="rt-tr -odd feeRow" role="row">
                                                                                    <input type="hidden" value="@item.IncidentReportID" name="IncidentHidden" id="IncidentHidden" />
                                                                                    <div class="rt-td text-center" role="gridcell" style="flex: 50 0 auto; width: 50px; max-width: 50px;">@item.IncidentReportID</div>
                                                                                    <div class="rt-td text-center" role="gridcell" style="flex: 100 0 auto; width: 100px;">@item.VehicleLicensePlateNo</div>
                                                                                    <div class="rt-td text-center" role="gridcell" style="flex: 200 0 auto; width: 200px; max-width: 200px;">@item.IncidentDateTime</div>
                                                                                    <div class="rt-td text-center" role="gridcell" style="flex: 100 0 auto; width: 100px;">@item.IncidentLocation</div>
                                                                                    <div class="rt-td text-center" role="gridcell" style="flex: 100 0 auto; width: 100px;">@item.SecurityOfficerName</div>
                                                                                    <div class="rt-td text-center" role="gridcell" style="flex: 100 0 auto; width: 100px; max-width: 100px;">@item.Status</div>
                                                                                    <div class="rt-td text-center" role="gridcell" style="flex: 80 0 auto; width: 80px; max-width: 80px;">
                                                                                        @*<a asp-action="Edit" id="editHere" class="mr-1 btn btn-primary btn-sm" data-toggle="modal" data-target="#edit_incident_report" onclick="fnEditIncidentReport()">
                                                                                                <i class="fa fa-pencil"></i>
                                                                                                <span>Edit</span>
                                                                                            </a>*@
                                                                                        @if (userRole.Role.RoleName == "ExternalAdmin")
                                                                                        {
                                                                                            <button type="button" id="editHere" disabled class="mr-1 btn btn-primary btn-sm editHere" data-target="#create_incident_report" onclick="fnEditIncidentReport()" data-toggle="modal"><i class="fa fa-pencil"></i> Edit</button>

                                                                                        }
                                                                                        else
                                                                                        {
                                                                                            <button type="button" id="editHere" class="mr-1 btn btn-primary btn-sm editHere" data-target="#create_incident_report" onclick="fnEditIncidentReport()" data-toggle="modal"><i class="fa fa-pencil"></i> Edit</button>
                                                                                        }
                                                                                    </div>
                                                                                    <div class="rt-td hideTableColumn" role="gridcell" style="flex: 0 0 auto; width: 0px; max-width: 0px;"></div>
                                                                                </div>
                                                                            </div>
                                                                        }
                                                                    }
                                                                }

                                                            </div>
