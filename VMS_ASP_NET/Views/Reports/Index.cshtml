                                                            <button class="mr-1 btn btn-success" disabled data-toggle="modal" data-target="#create_incident_report" onclick="fnGetIncidentReportModal()">
                                                                <i class="fa fa-plus"></i>
                                                                Create Incident Report
                                                            </button>
                                                        }
                                                        else
                                                        {
