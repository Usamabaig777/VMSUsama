<script type="text/javascript">
    
    $(document).ready(function () {
        alert('I am here');
        $('#SitePortion').hide();
        $('#GatePortion').hide();
        $('#GuardPortion').hide();
        $('#CompanyPortion').hide();
        $('#VehiclePortion').hide();
    });
    $('#reportID').change(function () {

        var reportIDVal = $('#reportID').val();
        alert(reportIDVal);

        if (reportIDVal == 'IndividualPortion') {
            $('#SitePortion').hide();
            $('#GatePortion').hide();
            $('#GuardPortion').hide();
            $('#CompanyPortion').hide();
            $('#VehiclePortion').hide();
        }
        else if (reportIDVal == 'SitePortion') {
            $('#GatePortion').hide();
            $('#GuardPortion').hide();
            $('#CompanyPortion').hide();
            $('#VehiclePortion').hide();
        }

    })
    function fnGetIncidentReportModal() {
        $.ajax({

            url: "/Reports/CreateIncidentReport/",
            success: function (result) {
                $("#IncidentReportSaveForm").html(result);
            }
        });
    };

    $('.editHere').click(function () {
        var IncidentID = $(this).closest(".feeRow").find("#IncidentHidden").val();
        //var IncidentID = $('#IncidentHidden').val();
        $.ajax({

            url: "/Reports/UpdateIncidentReport/",
            data: { 'id': IncidentID },
            success: function (result) {
                $("#IncidentReportSaveForm").html(result);
            }
        });

    })
</script>

