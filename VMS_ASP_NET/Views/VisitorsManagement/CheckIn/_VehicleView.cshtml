<script type="text/javascript">
    $(document).ready(function () {
        $('#seacrhedlpNo').hide();
        $("#LicensePlateNo").autocomplete({
            source: function (request, response) {
                $.ajax({
                    url: "/VisitorsManagement/GetVehiclesLicensePlateNo",
                    type: "POST",
                    dataType: "json",
                    data: { Prefix: request.term },
                    success: function (data) {
                        response($.map(data, function (item) {
                            $('#seacrhedlpNo').empty();
                            $.each(data, function (index, itemData) {
                                //alert("Index:" + itemData.VehicleID + ", Value:" + itemData.LicensePlateNo);
                                var lpNoList = "<option value = '" + itemData.VehicleID + "'>" + itemData.LicensePlateNo + "</option>";
                                //var app = "<li value=''>" + item.LicensePlateNo +"</li>";
                                $('#seacrhedlpNo').append(lpNoList)
                                $('#seacrhedlpNo').show();
                            });
                            //return { label: item.LicensePlateNo, value: item.LicensePlateNo };
                        }))
                    }
                })
            },
            messages: {
                noResults: "",
                results: function (count) {
                    return count + (count > 1 ? ' results' : ' result ') + ' found';
                }
            }
        });
    })
</script>